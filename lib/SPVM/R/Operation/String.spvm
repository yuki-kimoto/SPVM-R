class R::Operation::String extends R::Operation {
  
  use R::NDArray::String;
  
  # Class Methods
  static method new : R::Operation::String () {
    
    my $self = new R::Operation::String;
    
    return $self;
  }
  
  # Instance Methods
  method c : R::NDArray::String ($x_data : string[], $x_dim : int[] = undef) {
    
    my $x_ndarray = R::NDArray::String->new({dim => $x_dim, data => $x_data});
    
    return $x_ndarray;
  }
  
  method rep : R::NDArray::String ($x_ndarray : R::NDArray::String, $times : int) {
    return (R::NDArray::String)$self->SUPER::rep($x_ndarray, $times);
  }
  
}
