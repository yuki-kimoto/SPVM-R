class R::Operation::HalfFloat {
  
  use R::NDArray::HalfFloat;
  
  # Class Methods
  static method new : R::Operation::HalfFloat () {
    
    my $self = new R::Operation::HalfFloat;
    
    return $self;
  }
  
  # Instance Methods
  method c : R::NDArray::HalfFloat ($x_data : short[], $x_dim : int[] = undef) {
    
    my $x_ndarray = R::NDArray::HalfFloat->new({dim => $x_dim, data => $x_data});
    
    return $x_ndarray;
  }
  
}
