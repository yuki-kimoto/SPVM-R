class R::OP::StringBuffer {
  
  use R::NDArray::StringBuffer;
  use R::OP;
  use StringBuffer;
  
  # Class Methods
  static method c : R::NDArray::StringBuffer ($data : StringBuffer[], $dim : int[] = undef) {
    
    my $ndarray = R::NDArray::StringBuffer->new({dim => $dim, data => $data});
    
    return $ndarray;
  }
  
}
