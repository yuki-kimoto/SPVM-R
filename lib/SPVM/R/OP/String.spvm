class R::OP::String {
  
  use R::NDArray::String;
  use R::NDArray::StringBuffer;
  use R::NDArray::Time::Piece;
  use Time::Piece;
  use R::OP;
  use R::OP::StringBuffer;
  use R::OP::Time::Piece;
  
  # Class Methods
  static method c : R::NDArray::String ($data : string[], $dim : int[] = undef) {
    
    my $ndarray = R::NDArray::String->new({dim => $dim, data => $data});
    
    return $ndarray;
  }
  
}
