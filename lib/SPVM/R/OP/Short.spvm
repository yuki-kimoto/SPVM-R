class R::OP::Short {
  
  use R::NDArray::Short;
  use ShortList;
  use R::OP;
  
  # Class Methods
  static method c : R::NDArray::Short ($data : short[], $dim : int[] = undef) {
    
    my $ndarray = R::NDArray::Short->new({dim => $dim, data => $data});
    
    return $ndarray;
  }
  
}
