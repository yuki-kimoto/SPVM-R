  precompile static method scamul : R::NDArray::Int ($scalar_ndarray : R::NDArray::Int, $x_ndarray : R::NDArray::Int) {
    
    unless ($scalar_ndarray) {
      die "The n-dimensional array \$scalar_ndarray must be defined.";
    }
    
