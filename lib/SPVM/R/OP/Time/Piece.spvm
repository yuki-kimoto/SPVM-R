class R::OP::Time::Piece {
  
  use R::NDArray::Time::Piece;
  use R::OP;
  use Time::Piece;
  
  # Class Methods
  static method c : R::NDArray::Time::Piece ($data : Time::Piece[], $dim : int[] = undef) {
    
    my $ndarray = R::NDArray::Time::Piece->new({dim => $dim, data => $data});
    
    return $ndarray;
  }
  
}

