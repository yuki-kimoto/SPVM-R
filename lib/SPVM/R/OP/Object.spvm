class R::OP::Object {
  
  use R::OP;
  
  # Class Methods
  static method c : R::NDArray::Object ($x_data : object[], $x_dim : int[] = undef) {
    
    my $x_ndarray = R::NDArray::Object->new({dim => $x_dim, data => $x_data});
    
    return $x_ndarray;
  }
  
}
