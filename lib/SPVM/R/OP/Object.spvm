class R::OP::Object {
  
  use R::OP;
  
  # Class Methods
  static method c : R::NDArray::Object ($data : object[], $dim : int[] = undef) {
    
    my $ndarray = R::NDArray::Object->new({dim => $dim, data => $data});
    
    return $ndarray;
  }
  
}
