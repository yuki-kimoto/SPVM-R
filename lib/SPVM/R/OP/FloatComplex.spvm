  precompile static method scamul : R::NDArray::FloatComplex ($x_ndarray : R::NDArray::FloatComplex, $scalar_ndarray : R::NDArray::FloatComplex) {
    
    unless ($scalar_ndarray) {
      die "The n-dimensional array \$scalar_ndarray must be defined.";
    }
    
