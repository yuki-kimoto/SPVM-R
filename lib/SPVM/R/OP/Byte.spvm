class R::OP::Byte {
  
  use R::NDArray::Byte;
  
  # Class Methods
  static static method c : R::NDArray::Byte ($x_data : byte[], $x_dim : int[] = undef) {
    
    my $x_ndarray = R::NDArray::Byte->new({dim => $x_dim, data => $x_data});
    
    return $x_ndarray;
  }
  
}
