class R::NDArray::FloatComplexComplex {
  use Complex_2f;
  
  # Interface
  interface R::NDArray;
  
  # Fields
  has array : Complex_2f[];
  
  has dim : int[];
  
  # Class Methods
  static method new : R::NDArray::FloatComplex : ($dim : int[], $array : Complex_2f[] = undef) {
    
    my $array_length = -1;
    if ($array) {
      $array_length = @$array;
    }
    
    R::NDArray->init_partially($dim, $array_length);
    
    my $self = new R::NDArray::FloatComplex;
    
    if ($array) {
      $self->{array} = (Complex_2f[])copy $array;
    }
    else {
      $self->{array} = new Complex_2f[$array_length];
    }
    
    return $self;
  }
  
  # Instance Methods
  
  method dim : int[] {
    return (int)
  }
  
  method length : int {
    return $self->R::NDArray::length;
  }
  
  method to_array : Complex_2f[] () {
    return (Complex_2f[])copy $self->{array};
  }
  
  method get_array : Complex_2f[] () {
    return $self->{array};
  }
  
  method dim : int[] () {
    return (int[])copy $self->{dim};
  }
  
  method set_dim : void ($dim : int[]) {
    return $self->R::NDArray::set_dim($dim);
  }
}
