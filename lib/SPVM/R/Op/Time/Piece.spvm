class R::Op::Time::Piece {
  
  use R::NDArray::Time::Piece;
  use R::Op;
  
  # Class Methods
  static method c : R::NDArray::Time::Piece ($x_data : string[], $x_dim : int[] = undef) {
    
    my $x_ndarray = R::NDArray::Time::Piece->new({dim => $x_dim, data => $x_data});
    
    return $x_ndarray;
  }
  
}

