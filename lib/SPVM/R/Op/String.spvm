class R::Op::String extends R::Op {
  
  use R::NDArray::String;
  
  # Class Methods
  static method new : R::Op::String () {
    
    my $self = new R::Op::String;
    
    return $self;
  }
  
  # Instance Methods
  method c : R::NDArray::String ($x_data : string[], $x_dim : int[] = undef) {
    
    my $x_ndarray = R::NDArray::String->new({dim => $x_dim, data => $x_data});
    
    return $x_ndarray;
  }
  
}
