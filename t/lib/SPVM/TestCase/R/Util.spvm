  static method equals_dim : int () {
    
    {
      my $x_dim = [2, 3];
      my $y_dim = [2, 3];
      my $equals_dim = R::Util->equals_dim($x_dim, $y_dim);
      
      unless ($equals_dim) {
        return 0;
      }
    }
    
    {
      my $x_dim = [2, 3];
      my $y_dim = [2, 3, 1];
      my $equals_dim = R::Util->equals_dim($x_dim, $y_dim);
      
      if ($equals_dim) {
        return 0;
      }
    }
    
    {
      my $x_dim = new int[0];
      my $y_dim = new int[0];
      my $equals_dim = R::Util->equals_dim($x_dim, $y_dim);
      
      unless ($equals_dim) {
        return 0;
      }
    }
    
    # Exceptions
    {
      {
        my $x_dim = [2, 1, 3, 4];
        my $y_dim = [2, 1, 3, 4];
        eval { R::Util->equals_dim($x_dim, undef); }
        
        unless ($@) {
          return 0;
        }
      }
      
      {
        my $x_dim = [0, 1, 3, 4];
        my $y_dim = [2, 1, 3, 4];
        eval { R::Util->equals_dim($x_dim, $y_dim); }
        
        unless ($@) {
          return 0;
        }
      }
      
      {
        my $x_dim = [2, 1, 3, 4];
        my $y_dim = [0, 1, 3, 4];
        eval { R::Util->equals_dim($x_dim, $y_dim); }
        
        unless ($@) {
          return 0;
        }
      }
    }
    
    $@ = undef;
    
    return 1;
  }
  
  static method equals_dropped_dim : int () { return 1; }
}
